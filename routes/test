<label id="movie-rating">Rating (2)</label>
<div class="star-rating rating-sm rating-active"><div class="rating-container rating-fa" data-content=""><div class="rating-stars" data-content="" style="width: 0%;"></div><input name="rating" movie-id="19661" class="rating form-control hide" value="0" min="0" max="5" step="0.5" data-size="sm" data-symbol="" data-glyphicon="false" data-rating-class="rating-fa" data-show-clear="false"></div><div class="caption"><span class="label label-default">Not Rated</span></div></div>
<div class="clearfix"></div>
<script type="text/javascript">
    $(document).ready(function () {
        var $input = $('input.rating'), count = Object.keys($input).length;
        if (count > 0) {
            $input.rating();
        }
    });
    $('input[name=rating]').change(function () {
        if (is_login) {
            var mark = $(this).val();
            var movie_id = $(this).attr('movie-id');
            $.ajax({
                url: base_url + 'ajax/user_rating',
                method: 'POST',
                data: {movie_id: movie_id, mark: mark},
                dataType: 'json',
                success: function (data) {
                    if (data.status == 1) {
                        $('.mv-rating').html(data.html);
                    }
                }
            })
        } else {
            $('#pop-login').modal('show');
        }
    });
</script>
